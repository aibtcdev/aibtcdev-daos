{
  "name": "aibtcdev-daos",
  "version": "1.0.0",
  "description": "AI-powered Bitcoin DAOs",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:report": "vitest run -- --coverage --costs",
    "test:watch": "chokidar \"tests/**/*.ts\" \"contracts/**/*.clar\" -c \"npm run test:report\"",
    "dev": "node --loader ts-node/esm src/dev.ts",
    "dev:wrangler": "wrangler dev src/index.ts",
    "build": "vite build && vite build --ssr",
    "preview": "$npm_execpath run build && wrangler dev dist-server/index.js",
    "deploy": "$npm_execpath run build && wrangler deploy dist-server/index.js",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.7.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250508.0",
    "@cloudflare/vite-plugin": "^1.1.1",
    "@hirosystems/clarinet-sdk": "^3.0.1",
    "@hono/vite-build": "^1.6.1",
    "@stacks/transactions": "^7.0.6",
    "chokidar-cli": "^3.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vitest-environment-clarinet": "^2.3.0",
    "vite-plugin-ssr-hot-reload": "^0.5.0",
    "wrangler": "^4.14.4"
  }
}
